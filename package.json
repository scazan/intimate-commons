{
  "name": "intimate-commons",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "db:push": "npx prisma db push --force-reset && pnpm run db:generate",
    "db:generate": "npx prisma generate",
    "db:explore": "npx prisma studio",
    "db:reset": "pnpm run db:push && pnpm run db:generate && pnpm run db:seed",
    "db:seed": "npx prisma db seed",
    "db:clear": "pnpm db:push && pnpm db:seed",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "lint": "next lint"
  },
